{
  "manifest_version": 2,
  "name": "start-page",
  "description": "A momentum-inspired start page",
  "version": "0.1",
  "permissions": [
    "http://openweathermap.org/api"
  ],
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}
